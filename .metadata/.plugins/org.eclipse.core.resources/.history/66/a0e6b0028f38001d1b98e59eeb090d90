package statements;

public class S001_IF {
	public static void main(String[] args) {
		
		int a = 100;
		int b = 20;
		int c = 500;
		
		if(a>b)
		{
			if(a>c)
			{
				System.out.println("A is greater!!!");
			}
			else
			{
				System.out.println("c is greater");
			}
			
		}
		else
		{
			System.out.println("B is greater");
		}
		
		
		
		
	}
}
