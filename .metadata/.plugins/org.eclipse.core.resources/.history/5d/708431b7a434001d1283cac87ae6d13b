package oprators;

public class A001_Arithmatic {
	public static void main(String[] args) {
		
		
		// +, -, *, /, %
		
		/*
		 * System.out.println("tops"+10); System.out.println(10+10);
		 * System.out.println("tops"+10+10); System.out.println(10+10+"tops");
		 * 
		 * System.out.println(20-10); System.out.println(10*10);
		 * System.out.println(100/10); System.out.println(13%2);
		 */

		double a  =27;
		double b = 5;
		double result  =a/b;
		
		System.out.println(result);
		
		int i = 'B';
		System.out.println(i);
		
	}
}
