package basic;

public class Demo {
	public static void main(String[] args) {
		
		byte b = 127;
		short s = -32767;
		int i = 5689;
		long l = 5698;
		
		float f = 458.265f;
		double  d = 4589.2656565;
		
		char c = 'A';
		boolean bool = false;
				
		
		//System.out.println(a+" "+k+" "+i+c);
		//datatype casting 
		
		//implicite wideninig
		int k = 10;
		long k1 = k;
		
		//explicite nerrowing
		long x = 5689745698855855l;
		int x1 = (int) x;
		System.out.println(x1);
		
		
		
		
		
	}
}
